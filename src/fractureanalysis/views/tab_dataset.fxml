<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane 
    id="WhitePane" 
    prefHeight="400.0" 
    prefWidth="600.0" 
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="fractureanalysis.controller.AppController">
    
    <Label 
        GridPane.columnIndex="0"
        GridPane.rowIndex="0" 
        text="File name">
        
    </Label>
    
    <Label 
        fx:id="lFilename"
        GridPane.columnIndex="1"
        GridPane.rowIndex="0"
        text="">
    </Label>
    
    <Label 
        GridPane.columnIndex="0"
        GridPane.rowIndex="1" 
        text="Separator">
        
    </Label>
    
    <Label 
        fx:id="lSeparator"
        GridPane.columnIndex="1"
        GridPane.rowIndex="1"
        text="">
    </Label>
        
    <Label 
        GridPane.columnIndex="0"
        GridPane.rowIndex="2" 
        text="Number of columns">
        
    </Label>
    
    <Label 
        fx:id="lColumns"
        GridPane.columnIndex="1"
        GridPane.rowIndex="2"
        text="">
    </Label>
    
    <Label 
        GridPane.columnIndex="0"
        GridPane.rowIndex="3" 
        text="Number of rows">
        
    </Label>
    
    <Label 
        fx:id="lRows"
        GridPane.columnIndex="1"
        GridPane.rowIndex="3"
        text="">
    </Label>
    
    <Label 
        GridPane.columnIndex="0"
        GridPane.rowIndex="4" 
        text="Ap column">
        
    </Label>
    
    <Label 
        fx:id="lApColumn"
        GridPane.columnIndex="1"
        GridPane.rowIndex="4"
        text="">
    </Label>
    
    <Label 
        GridPane.columnIndex="0"
        GridPane.rowIndex="5" 
        text="Sp column">
        
    </Label>        
    
    <Label 
        fx:id="lSpColumn"
        GridPane.columnIndex="1"
        GridPane.rowIndex="5"
        text="">
    </Label>
    
</GridPane>
