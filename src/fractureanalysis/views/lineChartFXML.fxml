<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane 
    xmlns:fx="http://javafx.com/fxml/1" 
    id="gridLinePane" 
    prefHeight="400.0" 
    prefWidth="400.0"     
    fx:controller="fractureanalysis.controller.AppController" 
    alignment="CENTER"
    hgap="10"
    vgap="10">    
    <padding>
        <javafx.geometry.Insets ></javafx.geometry.Insets>
    </padding>
    
    <javafx.scene.chart.LineChart 
        fx:id="lineChart"
        GridPane.columnIndex="5" 
        GridPane.rowIndex="0" 
        GridPane.rowSpan="4" 
        GridPane.columnSpan="4" >
        <xAxis>
            <javafx.scene.chart.CategoryAxis 
                label="X"                 
                />                
        </xAxis>
        <yAxis>
            <javafx.scene.chart.CategoryAxis 
                label="Y"
                /> 
        </yAxis>        
    </javafx.scene.chart.LineChart>
       
</GridPane>
